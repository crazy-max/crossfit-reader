#!/bin/sh
echo "Start wineJdk..."

winePath=$1
jdkUrl=$2
echo "* WinePath: $winePath"
echo "* JdkUrl: $jdkUrl"

wget -c -O "$winePath/jdk.exe" --no-check-certificate --no-cookies --header "Cookie: oraclelicense=accept-securebackup-cookie" "$jdkUrl"
DISPLAY=:99.0 wine "C:\jdk.exe" /s INSTALLDIR=C:\jdk
